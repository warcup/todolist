{
  "name": "glass-todo-local",
  "version": "1.0.0",
  "description": "Local offline version of Glass Todo",
  "main": "server.js",
  "bin": {
    "glass-todo": "server.js"
  },
  "scripts": {
    "start": "node server.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.645.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.3",
    "ldapjs": "^3.0.7",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "sqlite3": "^5.1.7",
    "svg-captcha": "^1.4.0",
    "web-push": "^3.6.7"
  },
  "pkg": {
    "scripts": [
      "server.js",
      "server/**/*.js"
    ],
    "assets": [
      "public/**/*",
      "database.sqlite"
    ],
    "targets": [
      "node16-linux-x64",
      "node16-win-x64",
      "node16-macos-x64"
    ],
    "outputPath": "dist"
  }
}